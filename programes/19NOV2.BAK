#include <iostream>
#include<string>
using namespace std;

int main() {
    string str1;
    int i=0;
    int j=4;
    char temp;
    cin>>str1;
    
    while(i<=j){
        temp=str1[i];
        str1[i]=str1[j];
        str1[j]=temp;
        i++;
        j--;
    }
    cout<<str1;
    return 0;
}